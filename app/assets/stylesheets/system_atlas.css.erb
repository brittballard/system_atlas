* {
position:relative;
margin:0;
padding:0;
}

img {
border:0;
}

a,a:link,a:active,a:visited {
outline:none;
}

body {
background-color:#363d52;
background-image:url(<%=asset_path'Background-Repeat.png'%>);
background-repeat:repeat-x;
font-family:Arial;
font-size:12px;
}

#container-outer {
display:block;
width:960px;
margin:0 auto;
}

#header {
display:block;
width:960px;
overflow:hidden;
}

#header .utility {
display:block;
float:right;
height:25px;
line-height:25px;
background-color:#94b24d;
color:#FFF;
-webkit-box-shadow:0 0 3px 1px rgba(0,0,0,0.5);
-moz-box-shadow:0 0 3px 1px rgba(0,0,0,0.5);
box-shadow:0 0 3px 1px rgba(0,0,0,0.5);
-moz-border-radius-topleft:0;
-moz-border-radius-topright:0;
-moz-border-radius-bottomright:5px;
-moz-border-radius-bottomleft:5px;
-webkit-border-radius:0 0 5px 5px;
border-radius:0 0 5px 5px;
padding:3px 10px 0;
}

#header .utility a,#header .utility a:link,#header .utility a:active,#header .utility a:visited {
color:#FFF;
text-decoration:none;
}

#header .utility a:hover {
text-decoration:underline;
}

#header .utility .item {
display:block;
height:20px;
line-height:20px;
float:left;
vertical-align:middle;
}

#header .utility .divider {
display:block;
float:left;
line-height:20px;
height:12px;
vertical-align:middle;
width:2px;
border-right:1px solid #FFF;
margin:4px 10px 0;
}

#header .logo {
display:block;
float:left;
width:199px;
height:33px;
margin-top:35px;
}

#nav {
display:block;
width:960px;
height:50px;
-webkit-border-radius:5px;
-moz-border-radius:5px;
border-radius:5px;
-webkit-box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
-moz-box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#484c57',endColorstr='#2b2b2c',GradientType=0);
margin:17px 0 10px;
}

#nav ul {
list-style:none;
}

#nav ul li {
float:left;
border-right:1px solid #4d4d4d;
height:50px;
cursor:pointer;
}

#nav ul li a {
height:50px;
line-height:50px;
text-decoration:none;
color:#FFF;
font-size:14px;
padding:0 10px;
}

#nav ul li a .icon {
display:block;
float:left;
line-height:50px;
margin:10px 10px 0 20px;
}

#nav ul li a .label {
display:block;
float:left;
line-height:50px;
}

#nav ul li:hover,#nav ul li:first-child:hover {
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#272729',endColorstr='#353c50',GradientType=0);
}

#nav ul li:first-child:hover {
-moz-border-radius-topleft:5px;
-moz-border-radius-topright:0;
-moz-border-radius-bottomright:0;
-moz-border-radius-bottomleft:5px;
-webkit-border-radius:5px 0 0 5px;
border-radius:5px 0 0 5px;
}

#main-content {
background-color:#FFF;
width:940px;
-webkit-border-radius:3px;
-moz-border-radius:3px;
border-radius:3px;
-webkit-box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
-moz-box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
box-shadow:0 0 3px 1px rgba(0,0,0,0.3);
padding:10px;
}

.section-header {
height:30px;
line-height:30px;
color:#FFF;
-moz-border-radius-topleft:3px;
-moz-border-radius-topright:3px;
-moz-border-radius-bottomright:0;
-moz-border-radius-bottomleft:0;
-webkit-border-radius:3px 3px 0 0;
border-radius:3px 3px 0 0;
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#2b415c',endColorstr='#141f2e',GradientType=0);
}

.section-header .icon {
height:30px;
line-height:30px;
display:block;
float:left;
padding:0 0 0 10px;
}

.section-header .label {
height:30px;
line-height:30px;
display:block;
float:left;
}

.section-content {
display:block;
border:1px solid #CCC;
-moz-border-radius-topleft:0;
-moz-border-radius-topright:0;
-moz-border-radius-bottomright:3px;
-moz-border-radius-bottomleft:3px;
-webkit-border-radius:0 0 3px 3px;
border-radius:0 0 3px 3px;
padding:10px;
}

.entity {
display:block;
background-color:#363d52;
height:50px;
color:#FFF;
-moz-border-radius-topleft:3px;
-moz-border-radius-topright:3px;
-moz-border-radius-bottomright:3px;
-moz-border-radius-bottomleft:3px;
-webkit-border-radius:3px 3px 3px 3px;
border-radius:3px 3px 3px 3px;
margin:5px;
padding:10px;
}

.entity-list {
list-style-type:none;
}

#relationship-manager {
display:inline-block;
width:468px;
}

.ui-state-highlight,.ui-widget-content .ui-state-highlight {
border:1px solid grey 50% top repeat-x;
}

.ui-state-highlight a,.ui-widget-content .ui-state-highlight a {
color:#363636;
}

.hidden {
visibility:hidden;
};
