*{margin: 0; padding: 0; position: relative;}
body {background-color: #363d52; background-image: url(<%= asset_path 'Background-Repeat.png' %>); background-repeat: repeat-x; font-family: Arial; font-size: 12px;}
img {border: 0;}
a, a:link, a:active, a:visited{outline: none;}

#container-outer{display: block; width: 960px; margin: 0 auto;}
#header{display: block; width: 960px; overflow: hidden;}
#header .utility{display: block; float: right; height: 25px; line-height: 25px; background-color: #94b24d; color: #FFF; padding: 3px 10px 0 10px; -webkit-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.5);-moz-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.5);box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.5);  -moz-border-radius-topleft: 0px;-moz-border-radius-topright: 0px;-moz-border-radius-bottomright: 5px;-moz-border-radius-bottomleft: 5px;-webkit-border-radius: 0px 0px 5px 5px;border-radius: 0px 0px 5px 5px; }
#header .utility a, #header .utility a:link, #header .utility a:active, #header .utility a:visited{color: #FFF; text-decoration: none;}
#header .utility a:hover{text-decoration: underline;}
#header .utility .item{display: block; height: 20px; line-height: 20px; float: left; vertical-align: middle;}
#header .utility .divider{display: block; float: left; line-height: 20px; height: 12px; vertical-align: middle; width: 2px; border-right: 1px solid #FFF; margin: 4px 10px 0 10px;}
#header .logo{display: block; float: left; width: 199px; height: 33px; margin-top: 35px;}

#nav{display: block; width: 960px; margin: 17px 0 10px 0; height: 50px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px; -webkit-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3);-moz-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3);box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3);background: #484c57; /* Old browsers */background: -moz-linear-gradient(top, #484c57 0%, #37393e 44%, #2b2b2c 100%); /* FF3.6+ */background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#484c57), color-stop(44%,#37393e), color-stop(100%,#2b2b2c)); /* Chrome,Safari4+ */background: -webkit-linear-gradient(top, #484c57 0%,#37393e 44%,#2b2b2c 100%); /* Chrome10+,Safari5.1+ */background: -o-linear-gradient(top, #484c57 0%,#37393e 44%,#2b2b2c 100%); /* Opera11.10+ */background: -ms-linear-gradient(top, #484c57 0%,#37393e 44%,#2b2b2c 100%); /* IE10+ */filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#484c57', endColorstr='#2b2b2c',GradientType=0 ); /* IE6-9 */background: linear-gradient(top, #484c57 0%,#37393e 44%,#2b2b2c 100%); /* W3C */}
#nav ul{list-style: none;}
#nav ul li{float: left; border-right: 1px solid #4d4d4d; height: 50px; cursor: pointer;}
#nav ul li a{height: 50px; line-height: 50px; padding: 0 10px; text-decoration: none; color: #FFF; font-size: 14px;}
#nav ul li a .icon{display: block; float: left; line-height: 50px; margin: 10px 10px 0 20px;}
#nav ul li a .label{display: block; float: left; line-height: 50px;}
#nav ul li:hover, #nav ul li:first-child:hover{background: #272729; /* Old browsers */background: -moz-linear-gradient(top, #272729 0%, #37393e 44%, #353c50 99%); /* FF3.6+ */background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#272729), color-stop(44%,#37393e), color-stop(99%,#353c50)); /* Chrome,Safari4+ */background: -webkit-linear-gradient(top, #272729 0%,#37393e 44%,#353c50 99%); /* Chrome10+,Safari5.1+ */background: -o-linear-gradient(top, #272729 0%,#37393e 44%,#353c50 99%); /* Opera11.10+ */background: -ms-linear-gradient(top, #272729 0%,#37393e 44%,#353c50 99%); /* IE10+ */filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#272729', endColorstr='#353c50',GradientType=0 ); /* IE6-9 */background: linear-gradient(top, #272729 0%,#37393e 44%,#353c50 99%); /* W3C */}
#nav ul li:first-child:hover{-moz-border-radius-topleft: 5px;-moz-border-radius-topright: 0px;-moz-border-radius-bottomright: 0px;-moz-border-radius-bottomleft: 5px;-webkit-border-radius: 5px 0px 0px 5px;border-radius: 5px 0px 0px 5px; }

#main-content{background-color: #FFF; width: 940px; padding: 10px; -webkit-border-radius: 3px;-moz-border-radius: 3px;border-radius: 3px; -webkit-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3);-moz-box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3);box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.3); }

.section-header{height: 30px; line-height: 30px; color: #FFF; -moz-border-radius-topleft: 3px;-moz-border-radius-topright: 3px;-moz-border-radius-bottomright: 0px;-moz-border-radius-bottomleft: 0px;-webkit-border-radius: 3px 3px 0px 0px;border-radius: 3px 3px 0px 0px; background: #2b415c; /* Old browsers */background: -moz-linear-gradient(top, #2b415c 0%, #212e41 44%, #141f2e 100%); /* FF3.6+ */background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#2b415c), color-stop(44%,#212e41), color-stop(100%,#141f2e)); /* Chrome,Safari4+ */background: -webkit-linear-gradient(top, #2b415c 0%,#212e41 44%,#141f2e 100%); /* Chrome10+,Safari5.1+ */background: -o-linear-gradient(top, #2b415c 0%,#212e41 44%,#141f2e 100%); /* Opera11.10+ */background: -ms-linear-gradient(top, #2b415c 0%,#212e41 44%,#141f2e 100%); /* IE10+ */filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2b415c', endColorstr='#141f2e',GradientType=0 ); /* IE6-9 */background: linear-gradient(top, #2b415c 0%,#212e41 44%,#141f2e 100%); /* W3C */}
.section-header .icon{height: 30px; line-height: 30px; display: block; float: left; padding: 0 0 0 10px;}
.section-header .label{height: 30px; line-height: 30px; display: block; float: left;}

.section-content{display: block; border: 1px solid #CCC; padding: 10px; -moz-border-radius-topleft: 0px;-moz-border-radius-topright: 0px;-moz-border-radius-bottomright: 3px;-moz-border-radius-bottomleft: 3px;-webkit-border-radius: 0px 0px 3px 3px;border-radius: 0px 0px 3px 3px; }