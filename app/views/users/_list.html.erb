<table>
  <tr>
    <th>
      Login
    <th>
    <th>
      Email
    <th>
    <th>
      Actions
    </th>
  </tr>
  <% users.each do |user| %>
    <tr>
      <td>
        <%= user.login %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= link_to("Edit", edit_user_path(user)) %>
        <%= link_to("Delete", edit_user_path(user, user_path(user)), :method => :delete) %>
      </td>
    </tr>
  <% end %>
</table>