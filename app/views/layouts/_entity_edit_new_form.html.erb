<%= form_for(form_object) do |form| %>
  <%= form.fields_for(:entity, entity_object) do |entity_fields| %>
    <%= render(:partial => 'entities/form', :locals => { :form => entity_fields, :select_object => entity_object_attributes_string }) %>
  <% end %>
  <%= render(:partial => form, :locals => { :form => form })%>
  <% if entity_object.new_record? %>
    <%= form.submit :create %>
  <% else %>
    <%= form.submit :edit %>
  <% end %>
<% end %>